{
    "$schema": "https://turborepo.org/schema.json",
    "remoteCache": {
        "signature": true
    },
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                ".next/**"
            ]
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        }
    }
}